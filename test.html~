<html>
	<head>
		<script src="scriptLoader.js"></script>
		<script src="scriptLoaderConfig.js"></script>
		<script type="text/javascript" >
			var start = Date.now();
			var stop;
			var time;
			var scripts = [];
			var i;
			for(i = 0; i < 100; i++){
				scripts[i]="test4.js";
			}
			scriptLoaderConfig.scripts = scripts;
			var loader = new ScriptLoader(scriptLoaderConfig,function(){stop = Date.now();time = stop - start;alert("scriptLoaderFinished -- " + time + " milliseconds");},function(message){alert(message);});
				
		</script>
	</head>
	<body>
	
	</body>
</html>